{
    "name": "devthis/console-logg",
    "description": "Standard Laravel logger output for console applications",
    "keywords": [
        "laravel",
        "symfony",
        "console",
        "psr3",
        "logger",
        "channels"
    ],
    "authors": [
        {
            "name": "Mitchell Macphrson",
            "email": "mitchell@devthis.com.au"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/log": "^6.0",
        "illuminate/console": "^6.0",
        "illuminate/support": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevThis\\ConsoleLogg\\TodolistServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "DevThis\\ConsoleLogg\\": "src/"
        }
    }
}
